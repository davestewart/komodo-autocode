<?xml version="1.0"?>
<RDF xmlns="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:em="http://www.mozilla.org/2004/em-rdf#">

    <Description about="urn:mozilla:install-manifest">

        <em:id>autocode@xjsfl.com</em:id>
        <em:name>AutoCode</em:name>
        <em:version>1.0</em:version>
        <em:description>Adds various features to the Komodo, including: JavaScript console &amp; debugging commands, TAB-completion on abbreviations, TAB-completion for class, function and variable DocComments, including optional column-layout, ALT-Click insertion of paths from the Places panel, and a larger auto-completion box. See Preferences &gt; Editor &gt; AutoCode to update settings.</em:description>
        <em:creator>Dave Stewart</em:creator>
        <em:homepageURL>http://www.xjsfl.com/support/setup/komodo/autocode</em:homepageURL>
		<em:iconURL>chrome://autocode/skin/icon.png</em:iconURL>
        <em:type>2</em:type> <!-- type=extension -->
		<em:unpack>true</em:unpack>

        <em:targetApplication>
            <Description>
                <!-- Komodo IDE's uuid -->
                <em:id>{36E66FA0-F259-11D9-850E-000D935D3368}</em:id>
                <em:minVersion>7</em:minVersion>
                <em:maxVersion>99</em:maxVersion>
            </Description>
        </em:targetApplication>
        <em:targetApplication>
            <Description>
                <!-- Komodo Edit's uuid -->
                <em:id>{b1042fb5-9e9c-11db-b107-000d935d3368}</em:id>
                <em:minVersion>7</em:minVersion>
                <em:maxVersion>99</em:maxVersion>
            </Description>
        </em:targetApplication>

    </Description>

</RDF>
